{
  "name": "sps-delegate-dom",
  "version": "0.1.0",
  "description": "The reference delegate implementation for consuming the Scalable Pixel Streaming frontend library",
  "main": "index.ts",
  "scripts": {
    "build": "npx webpack",
    "watch": "npx webpack --watch",
    "start": "npx webpack && open-cli ./dist/index.html",
    "serve": "webpack serve",
    "symlink": "cd ../../library && npm link && cd ../delegates/dom && npm link @tensorworks/libspsfrontend",
    "build-all": "cd ../../library && npm run build && cd ../delegates/dom && npm run symlink && npm run build"
  },
  "author": "TensorWorks Pty Ltd",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20221111.1",
    "@tensorworks/libspsfrontend": "0.1.4",
    "bootstrap": "^5.0.2",
    "css-loader": "^6.2.0",
    "file-loader": "^6.2.0",
    "grpc_tools_node_protoc_ts": "^5.3.3",
    "grpc-tools": "^1.12.4",
    "html-loader": "^4.1.0",
    "html-webpack-plugin": "^5.3.2",
    "http-proxy-middleware": "^2.0.6",
    "image-webpack-loader": "^8.1.0",
    "mini-css-extract-plugin": "^2.1.0",
    "open-cli": "^7.0.0",
    "style-loader": "^3.2.1",
    "svg-url-loader": "^8.0.0",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "vitest": "^0.27.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.11.1",
    "wrangler": "2.7.1"
  },
  "dependencies": {
    "@bufbuild/buf": "^1.15.0",
    "@esbuild/linux-x64": "^0.17.3",
    "@grpc/grpc-js": "^1.8.12",
    "@improbable-eng/grpc-web": "^0.15.0",
    "@popperjs/core": "^2.11.6",
    "grpc-web": "^1.4.2",
    "nodemon": "^2.0.21",
    "protobufjs": "^7.2.2",
    "protoc-gen-grpc-web": "^1.4.1",
    "protoc-gen-ts": "^0.8.5",
    "svg-inline-loader": "^0.8.2",
    "ts-proto": "^1.141.1"
  }
}
